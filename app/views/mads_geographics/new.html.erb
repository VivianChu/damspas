<script type="text/javascript">

$(document).ready(function(){
  getSingleAutoCompleteList();
});

</script>

<h1>Create a new MADS Geographic</h1>
<%= form_for @mads_geographic, :html => { :onsubmit => 'removeEmptyFields();'} do |f| %>
	<%= render 'error_messages' %>

  <div id="subjectField">
  <%= label_tag 'mads_geographic[name]', "Name" %>
  <%= text_field_tag 'mads_geographic[name]', '', required: true, class: "typeahead" %>
</div>

  <%= label_tag 'mads_geographic[externalAuthority]', "ExternalAuthority" %>
  <%= text_field_tag 'mads_geographic[externalAuthority]', '', class: "input-block-level" %>
  <%= f.fields_for :geographicElement do |te| %>
    <%= te.label :elementValue, "Element Value" %>

            <%= te.text_field :elementValue, class: "eleTypeahead", :required=>true %>

  <% end %>
  <br>
  <%= render :partial => 'shared/edit_fields/scheme_fields' , :locals => {:f => f}%>
  <br>

  <%= render :partial => 'shared/edit_fields/new_form_hidden_fields' , :locals => {:parent_id => params[:parent_id], :parent_class => params[:parent_class]}%>

  <%=f.submit "Submit", class: "btn btn-primary" %>
<%end%>
